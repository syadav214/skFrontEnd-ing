{
  "name": "basicelectron",
  "version": "1.0.0",
  "description": "a small and basic electron app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "debug": "electron --inspect=5858 .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-basic-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ElectroApp\"",
    "package-linux": "electron-packager . electron-basic-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syadav214/skFrontEnd-ing.git"
  },
  "keywords": [
    "electron",
    "js"
  ],
  "author": "Santoshkumar Yadav",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/syadav214/skFrontEnd-ing/issues"
  },
  "homepage": "https://github.com/syadav214/skFrontEnd-ing#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "electron": "^3.0.3",
    "electron-packager": "^13.0.1",
    "electron-reload": "^1.4.0",
    "electron-store": "^2.0.0"
  }
}
